{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "script-src 'self' blob:;"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/server.js"
    }
  ]
}
